{
  "env": {
    "es2021": true,
    "browser": true,
    "node": true,
    "cypress": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard",
    "prettier/standart",
    "prettier/react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
      "prettier/prettier": "error",
      "space-before-function-paren": "off"
  }
}
